{
  "id": "freewebnovel",
  "name": "FreeWebNovel",
  "base_url": "https://freewebnovel.com",
  "lang": "en",
  "version": "1.0",
  "author": "Yomu Team",
  "description": "Free web novel reading site with large collection of translated novels",
  "icon_url": "https://freewebnovel.com/favicon.ico",
  "selectors": {
    "search": {
      "novel_list": ".novel-item",
      "novel_title": ".novel-title",
      "novel_author": ".novel-author",
      "novel_cover": ".novel-cover img",
      "novel_url": ".novel-title a",
      "next_page": ".pagination .next"
    },
    "novel": {
      "title": "h1, h3",
      "author": ".novel-author",
      "description": ".novel-description, .description",
      "cover": ".novel-cover img",
      "status": ".novel-status",
      "genres": ".novel-genres .genre, .genres a",
      "chapter_list": ".chapter-list .chapter, .chapter-item",
      "chapter_title": "a",
      "chapter_url": "a",
      "chapter_date": ".chapter-date"
    },
    "chapter": {
      "content": ".chapter-content",
      "title": ".chapter-title",
      "next_chapter": ".next-chapter a",
      "prev_chapter": ".prev-chapter a"
    }
  },
  "urls": {
    "search": "/search?q={query}&page={page}",
    "popular": "/sort/most-popular?page={page}",
    "latest": "/latest?page={page}",
    "novel_detail": "{novel_url}"
  },
  "settings": {
    "delay_between_requests": 3000,
    "max_concurrent_requests": 1,
    "use_cloudflare_bypass": false,
    "custom_headers": {
      "User-Agent": "Mozilla/5.0 (Linux; Android 13) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/112.0.0.0 Mobile Safari/537.36"
    },
    "requires_login": false,
    "search_delay": 1500
  }
}
